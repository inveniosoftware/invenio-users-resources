{
  "mappings": {
    "_doc": {
      "dynamic": "strict",
      "dynamic_templates": [
          {
            "profile": {
              "path_match": "profile.*",
              "mapping": {
                "type": "keyword"
              }
            }
          }
      ],
      "properties": {
        "$schema": {
          "type": "keyword",
          "index": "false"
        },
        "id": {
          "type": "keyword"
        },
        "version_id": {
          "type": "integer",
          "index": "false"
        },
        "uuid": {
          "type": "keyword"
        },
        "created": {
          "type": "date"
        },
        "updated": {
          "type": "date"
        },
        "email": {
          "type": "keyword"
        },
        "username": {
          "type": "keyword"
        },
        "full_name": {
          "type": "text"
        }
      }
    }
  }
}
